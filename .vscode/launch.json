  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "python": "${workspaceFolder}/rag/venv/bin/python3",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Go: Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "console": "integratedTerminal",
      "preLaunchTask": "bun: build"
    }
  ]
}
